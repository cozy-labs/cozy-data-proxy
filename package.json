{
  "name": "cozy-desktop",
  "version": "0.16.0",
  "description": "File Synchronization Client for Cozy Cloud",
  "homepage": "https://github.com/cozy-labs/cozy-desktop",
  "author": "Cozy Cloud <contact@cozycloud.cc> (https://cozycloud.cc/)",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/cozy-labs/cozy-desktop/issues"
  },
  "main": "./lib/app.js",
  "bin": {
    "cozy-desktop": "bin/cozy-desktop"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/cozy-labs/cozy-desktop.git"
  },
  "keywords": [
    "desktop",
    "data",
    "sync",
    "files",
    "cozy"
  ],
  "scripts": {
    "lint": "coffeelint src/ test/",
    "test": "installed-check && npm run test-unit && npm run test-integration",
    "test-unit": "NODE_ENV=test mocha test/unit/",
    "test-integration": "NODE_ENV=test mocha test/integration/",
    "build": "coffee -o lib/ src/",
    "clean": "rm -rf lib/ && rm -rf tmp/",
    "size": "t=\"$(npm pack .)\"; wc -c \"${t}\"; tar tvf \"${t}\"; rm \"${t}\";"
  },
  "dependencies": {
    "async": "2.1.4",
    "chokidar": "1.6.1",
    "commander": "2.9.0",
    "cozy-device-sdk": "1.4.0",
    "fs-extra": "1.0.0",
    "lodash.clone": "4.5.0",
    "lodash.find": "4.6.0",
    "lodash.isequal": "4.4.0",
    "lodash.pick": "4.4.0",
    "micromatch": "2.3.11",
    "mime": "1.3.4",
    "node-uuid": "1.4.7",
    "path-extra": "4.0.0",
    "pouchdb": "6.1.0",
    "printit": "0.1.20",
    "progress": "1.1.8",
    "read": "1.0.7",
    "readdirp": "2.1.0",
    "request-json-light": "0.5.25"
  },
  "devDependencies": {
    "coffee-script": "1.12.1",
    "coffeelint": "1.16.0",
    "del": "2.2.2",
    "faker": "3.1.0",
    "installed-check": "^2.1.1",
    "jsverify": "0.7.4",
    "lodash.uniq": "4.5.0",
    "mocha": "3.2.0",
    "pouchdb-server": "1.2.1",
    "should": "11.1.2",
    "sinon": "1.17.6"
  }
}

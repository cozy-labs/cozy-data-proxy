{
  "name": "cozy-desktop",
  "version": "0.16.0",
  "description": "File Synchronization Client for Cozy Cloud",
  "homepage": "https://github.com/cozy-labs/cozy-desktop",
  "author": "Cozy Cloud <contact@cozycloud.cc> (https://cozycloud.cc/)",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/cozy-labs/cozy-desktop/issues"
  },
  "main": "./lib/app.js",
  "bin": {
    "cozy-desktop": "bin/cozy-desktop"
  },
  "repository": "git://github.com/cozy-labs/cozy-desktop.git",
  "keywords": [
    "desktop",
    "data",
    "sync",
    "files",
    "cozy"
  ],
  "scripts": {
    "fauxton": "babel-node ./src/bin/fauxton.js",
    "flow": "flow status --quiet --show-all-errors",
    "lint": "npm run standard && npm run flow",
    "standard": "./node_modules/.bin/standard './src/**/*.js' './test/**/*.js'",
    "test": "installed-check && npm run test-unit && npm run test-integration",
    "test-coverage": "installed-check && npm run test-unit-coverage && npm run test-integration",
    "test-unit": "NODE_ENV=test mocha test/unit/",
    "test-unit-coverage": "NODE_ENV=test nyc mocha test/unit/",
    "test-integration": "NODE_ENV=test mocha test/integration/",
    "build": "babel --source-maps --out-dir lib/ src/",
    "clean": "rm -rf lib/ && rm -rf tmp/",
    "size": "t=\"$(npm pack .)\"; wc -c \"${t}\"; tar tvf \"${t}\"; rm \"${t}\";",
    "watch": "npm run build -- --watch"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ]
  },
  "dependencies": {
    "async": "2.1.4",
    "chokidar": "1.6.1",
    "commander": "2.9.0",
    "cozy-device-sdk": "1.4.0",
    "fs-extra": "1.0.0",
    "lodash.clone": "4.5.0",
    "lodash.find": "4.6.0",
    "lodash.isequal": "4.4.0",
    "lodash.pick": "4.4.0",
    "micromatch": "2.3.11",
    "mime": "1.3.4",
    "node-fetch": "^1.6.3",
    "node-uuid": "1.4.7",
    "pouchdb": "6.1.0",
    "printit": "0.1.20",
    "progress": "1.1.8",
    "read": "1.0.7",
    "readdirp": "2.1.0",
    "request-json-light": "0.5.25",
    "should-sinon": "^0.0.5",
    "sinon-stub-promise": "^4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-autobind-class-methods": "^2.1.1",
    "babel-plugin-istanbul": "^3.1.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "del": "2.2.2",
    "eslint-plugin-flowtype": "^2.30.0",
    "faker": "3.1.0",
    "flow-bin": "^0.38.0",
    "installed-check": "^2.1.1",
    "istanbul": "^0.4.5",
    "jsverify": "0.7.4",
    "lodash.uniq": "4.5.0",
    "mocha": "3.2.0",
    "nyc": "^10.0.0",
    "pouchdb-server": "^2.0.0",
    "should": "11.1.2",
    "sinon": "1.17.6",
    "source-map-support": "^0.4.8",
    "standard": "^8.6.0"
  }
}

{
  "name": "cozy-desktop",
  "version": "3.3.1",
  "description": "File Synchronization Client for Cozy Cloud",
  "homepage": "https://docs.cozy.io/en/mobile/desktop.html",
  "author": "Cozy Cloud <contact@cozycloud.cc> (https://cozycloud.cc/)",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/cozy-labs/cozy-desktop/issues"
  },
  "main": "./lib/app.js",
  "bin": {
    "cozy-desktop": "bin/cozy-desktop"
  },
  "repository": "git://github.com/cozy-labs/cozy-desktop.git",
  "logo": "https://raw.githubusercontent.com/cozy-labs/cozy-desktop/master/gui/images/icon.png",
  "keywords": [
    "desktop",
    "data",
    "sync",
    "files",
    "cozy"
  ],
  "files": [
    "bin/cozy-desktop",
    "doc/",
    "lib/",
    "CHANGELOG.md",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "bootstrap": "cd .. && yarn test:setup",
    "bootstrap:remote": "babel-node ./dev/remote/generate-test-env.js",
    "build": "babel --source-maps --out-dir lib/ src/",
    "capture": "env-cmd .env.test babel-node ./dev/capture.js",
    "clean": "rimraf lib/ tmp/",
    "eslint": "./node_modules/.bin/eslint './src/**/*.js' './test/**/*.js'",
    "fauxton": "babel-node ./src/bin/fauxton.js",
    "flow": "flow status --quiet --show-all-errors",
    "lint": "npm run eslint && npm run flow",
    "mocha": "cross-env COZY_DESKTOP_HEARTBEAT=1000 env-cmd .env.test mocha",
    "mocha:coverage": "env-cmd .env.test nyc mocha",
    "release:build": "yarn release:check && yarn && yarn run clean && yarn build",
    "release:check": "babel-node ./dev/check-release-branch.js",
    "repl": "cross-env DEBUG=1 COZY_DESKTOP_HEARTBEAT=5000 babel-node ./src/bin/repl.js",
    "size": "t=\"$(npm pack .)\"; wc -c \"${t}\"; tar tvf \"${t}\"; rm \"${t}\";",
    "start": "cross-env COZY_DESKTOP_HEARTBEAT=5000 babel-node ./src/bin/cli.js",
    "test": "npm run test:unit && npm run test:integration",
    "test:coverage": "npm run test:unit:coverage && npm run test:integration",
    "test:integration": "yarn mocha -- test/integration",
    "test:performance": "yarn mocha -- test/performance",
    "test:unit": "yarn mocha -- test/unit/",
    "test:unit:coverage": "yarn mocha:coverage -- test/unit/",
    "watch": "npm run build -- --watch"
  },
  "dependencies": {
    "async": "^2.1.5",
    "babel-polyfill": "^6.23.0",
    "bluebird": "^3.5.0",
    "btoa": "1.1.2",
    "bunyan": "^1.8.10",
    "chokidar": "1.7.0",
    "commander": "2.9.0",
    "cozy-client-js": "^0.3.13",
    "fs-extra": "^3.0.0",
    "isomorphic-fetch": "2.2.1",
    "lodash": "^4.17.4",
    "micromatch": "3.0.2",
    "mime": "^1.3.4",
    "node-fetch": "1.7.0",
    "opn": "5.0.0",
    "pouchdb": "^6.2.0",
    "pouchdb-find": "^6.2.0",
    "progress": "^2.0.0",
    "read": "1.0.7",
    "readdirp": "2.1.0",
    "request-json-light": "0.5.25",
    "trash": "^4.0.1",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-autobind-class-methods": "^5.0.1",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-es2017": "^6.22.0",
    "cheerio": "^0.22.0",
    "cross-env": "^5.0.0",
    "del": "^2.2.2",
    "env-cmd": "^5.0.0",
    "eslint": "^3.17.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-flowtype": "^2.30.3",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.0",
    "faker": "^4.1.0",
    "flow-bin": "^0.47.0",
    "flow-typed": "^2.0.0",
    "istanbul": "^0.4.5",
    "jsverify": "^0.8.0",
    "mocha": "^3.4.2",
    "mocha-clean": "^1.0.0",
    "node-uuid": "^1.4.8",
    "nyc": "^11.0.1",
    "pouchdb-server": "^2.3.4",
    "rimraf": "^2.6.1",
    "should": "^11.2.1",
    "should-sinon": "^0.0.5",
    "sinon": "^2.3.1",
    "source-map-support": "^0.4.11"
  }
}
